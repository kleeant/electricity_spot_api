{
  "dependencies": {
    "@sinclair/typebox": "^0.32.20",
    "axios": "^1.6.8",
    "axios-curlirize": "^1.3.7",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-jwt": "^8.4.1",
    "express-openapi-validator": "^5.1.6",
    "fast-xml-parser": "^4.3.6",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.13.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/axios-curlirize": "^1.3.5",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/minimist": "^1.2.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "copyfiles": "^2.4.1",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-standard": "^12.0.2",
    "typescript": "^5.4.3"
  },
  "name": "base_api_typescript",
  "description": "base project for an api",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky",
    "start": "npm run build && node dist/src/index.js",
    "dev": "npm run docker:local:up && nodemon --exec ts-node src/index.ts",
    "build": "rimraf dist && tsc && npm run copy:openapi",
    "copy:openapi": "npx copyfiles -f \"./src/infrastructure/server/server.openapi.yaml\" \"./dist/src/infrastructure/server\"",
    "lint": "ts-standard --fix",
    "docker:local:up": "docker-compose --env-file .env -f docker/docker-compose.local.yml up -d",
    "make:migration": "npx knex migration:make",
    "make:seed": "npx knex seed:make",
    "test": "jest",
    "test:all": "npm run docker:local:up && npm run test",
    "types:delete": "rimraf src/lib/types/generated.d.ts",
    "types:generate": "npm run types:delete && npx openapi-typescript ./src/infrastructure/server/server.openapi.yaml -o ./src/lib/types/generated.d.ts && npm run lint -- ./src/lib/types/generated.d.ts",
    "run:update:prices": "node dist/src/infrastructure/cli/cli.updateSpotPrices.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kleeant/base_api.git"
  },
  "author": "Antti Kleemola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kleeant/base_api/issues"
  },
  "homepage": "https://github.com/kleeant/base_api#readme"
}
